{
    "name": "kl83/yii2-file-storage",
    "description": "Yii2 file storage",
    "keywords": ["yii2", "file", "storage"],
    "license": "BSD-3-Clause",
    "version": "0.0.1",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Aleksandr Solovyov",
            "email": "solovyov.a.a.19@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "^2.0.0"
    },
    "require-dev": {
        "flow/jsonpath": "@dev"
    },
    "autoload": {
        "psr-4": { "kl83\\filestorage\\": "" }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    }
}
